  "scripts": {
    "dev": "nodemon --exec \"babel-node lib --presets @babel/env\"",
    "dev:cron": "babel-node lib/cron --presets @babel/env",
    "dev:worker": "babel-node lib/worker --presets @babel/env",
    "build": "rimraf dist && babel lib -s -D -d dist --presets @babel/env",
    "start": "node dist",
    "start:cron": "node dist/cron",
    "start:worker": "node dist/worker",
    "migration": "babel-node scripts/migration --presets @babel/env",
    "seed": "babel-node scripts/seed --presets @babel/env",
    "format": "prettier --write \"**/*.js\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^3.10.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "handlebars": "^4.1.2",
    "jwt-simple": "^0.5.6",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.0",
    "onesignal-node": "^2.1.1",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
